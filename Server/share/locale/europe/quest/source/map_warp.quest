					elseif empire == 3 then
						sub_set2 = select(locale_quest(6449), locale_quest(6450), locale_quest(6475), locale_quest(1650))
					end
					if sub_set2 == 4 then
						return
					end
				end
				if sub_set2 != 4 then
					if pc.get_gold() < cost then
						say_title(locale_quest(6444))
						say(locale_quest(6435))
						say_reward(string.format(locale_quest(6437), cost))
					else
						if sub_set2 == 1 then
							say_title(locale_quest(6444))
							say(locale_quest(6451))
							wait()
							if pc.is_dead() == false then
								if empire == 1 then
									pc.warp(474300, 954800)
								elseif empire == 2 then
									pc.warp(63800, 166400)
								elseif empire == 3 then
									pc.warp(959900, 269200)
								end
							else
								return
							end
						elseif sub_set2 == 2 then
							say_title(locale_quest(6444))
							say(locale_quest(6452))
							wait()
							if pc.is_dead() == false then
								if empire == 1 then
									pc.warp(353100, 882900)
								elseif empire == 2 then
									pc.warp(145500, 240000)
								elseif empire == 3 then
									pc.warp(863900, 246000)
								end
							else
								return
							end
						elseif sub_set2 == 3 then
							say_title(locale_quest(6444))
							say(locale_quest(6452))
							wait()
							if pc.is_dead() == false then
								if empire == 1 then
									pc.warp(287800, 799700)
								elseif empire == 2 then
									pc.warp(275500, 800000)
								elseif empire == 3 then
									pc.warp(277000, 788000)
								end
							else
								return
							end
						end
						pc.changegold(-cost)
					end
				end
			end
		end

		when 9012.chat.locale_quest(6453) with pc.get_level() >= 90 and pc.get_map_index()!= 301 begin
			say_title(mob_name(9012))
			say(locale_quest(6454))
			local select1 = select(locale_quest(6555), locale_quest(6556))
			if select1 == 1 then
				say_title(mob_name(9012))
				say(locale_quest(6455))
				local cost = 50000
				if pc.get_level() >= 90 then
					cost = cost + ((pc.get_level() - 90) * 10000)
				end
				say_reward(string.format(locale_quest(6456), cost))
				local select2 = select(locale_quest(6555), locale_quest(6556))
				if select2 == 1 and pc.get_level() >= 90 and pc.get_gold() >= cost then
					pc.changegold(-cost)
					pc.warp(1104300, 1788500)
				else
					if pc.get_gold() < cost then
						say_title(locale_quest(6444))
						say(locale_quest(6435))
						say_reward(string.format(locale_quest(6437), cost))
						return
